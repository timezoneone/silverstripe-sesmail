{
  "name": "symbiote/silverstripe-sesmail",
  "description": "Custom Mailer type that sends mail via Amazon's SES",
  "type": "silverstripe-vendormodule",
  "keywords": ["silverstripe", "email", "amazon", "ses"],
  "license": "BSD-3-Clause",
  "authors": [{
    "name": "Marcus Nyeholt",
    "email": "marcus@symbiote.com.au"
  }],
  "require":
  {
    "silverstripe/framework": "^4 || ^5",
    "aws/aws-sdk-php": "~3.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Symbiote\\SilverStripeSesmail\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "replace": {
    "silverstripe-australia/silverstripe-sesmail": "self.version"
  }
}
